{
  "name": "e-taxi",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl -e --config moleculer.config.ts services",
    "start": "moleculer-runner --config dist/moleculer.config.js",

    "dc:dev-up": "docker-compose -f docker/dev/docker-compose.yml up --build -d",
    "dc:dev-down": "docker-compose -f docker/dev/docker-compose.yml down",
    "dc:logs": "docker-compose logs -f"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/amqplib": "^0.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/dotenv-flow": "^3.2.0",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.9",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^29.3.1",
    "jest-chain": "^1.1.6",
    "jest-extended": "^4.0.0",
    "moleculer-repl": "^0.7.3",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/node-geocoder": "^4.2.1",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv-flow": "^3.2.0",
    "dotenv-flow-cli": "^1.0.0",
    "ioredis": "^5.0.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "moleculer": "^0.14.27",
    "moleculer-db": "^0.8.21",
    "moleculer-db-adapter-mongo": "^0.4.16",
    "moleculer-io": "^2.2.0",
    "moleculer-web": "^0.10.5",
    "mongodb": "^5.7.0",
    "nats": "^2.7.1",
    "node-geocoder": "^4.2.0",
    "ol": "^7.4.0",
    "queue-typescript": "^1.0.1",
    "twilio": "^4.14.1",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">= 16.x.x"
  }
}
